{
  "name": "awsome-json-editor",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "npm run build-ts && npm run tslint",
    "serve": "PORT=4200 node dist/index.js",
    "start": "npm run serve",
    "start-local": "npm run build && npm run serve",
    "watch-node": "nodemon dist/index.js",
    "watch": "concurrently -k -p \"[{name}]\" -n \"Sass,TypeScript,Node\" -c \"yellow.bold,cyan.bold,green.bold\" \"npm run watch-ts\" \"npm run watch-node\"",
    "test": "jest --forceExit --coverage --verbose",
    "watch-test": "npm run test -- --watchAll",
    "build-ts": "tsc",
    "watch-ts": "tsc -w",
    "tslint": "tslint --fix -c tslint.json -p tsconfig.json",
    "debug": "npm run build && npm run watch-debug",
    "serve-debug": "nodemon --inspect dist/index.js",
    "watch-debug": "concurrently -k -p \"[{name}]\" -n \"Sass,TypeScript,Node\" -c \"yellow.bold,cyan.bold,green.bold\" \"npm run watch-ts\" \"npm run serve-debug\""
  },
  "dependencies": {
    "@types/jsoneditor": "^5.19.0",
    "bcrypt-nodejs": "^0.0.3",
    "bluebird": "^3.5.1",
    "body-parser": "~1.18.2",
    "compression": "^1.7.2",
    "cookie-parser": "~1.4.3",
    "debug": "~2.6.9",
    "dotenv": "^5.0.1",
    "ejs": "*",
    "errorhandler": "^1.5.0",
    "express": "~4.15.5",
    "express-session": "^1.15.6",
    "express-validator": "^5.2.0",
    "inversify": "^4.13.0",
    "inversify-express-utils": "^6.0.0",
    "jsoneditor": "*",
    "lodash": "^4.17.10",
    "lusca": "^1.6.0",
    "morgan": "~1.9.0",
    "nodemailer": "^4.6.5",
    "pug": "^2.0.3",
    "puppeteer": "^1.4.0",
    "reflect-metadata": "^0.1.12",
    "request": "^2.87.0",
    "request-promise": "^4.2.2",
    "serve-favicon": "~2.4.5",
    "serverless": "^1.27.3",
    "winston": "^2.4.2",
    "json-beautify": ""
  },
  "devDependencies": {
    "@types/async": "^2.0.49",
    "@types/bcrypt-nodejs": "^0.0.30",
    "@types/bluebird": "^3.5.20",
    "@types/body-parser": "^1.17.0",
    "@types/compression": "^0.0.36",
    "@types/crypto-js": "^3.1.43",
    "@types/dotenv": "^4.0.3",
    "@types/errorhandler": "^0.0.32",
    "@types/express": "^4.11.1",
    "@types/express-session": "^1.15.8",
    "@types/jest": "^22.2.3",
    "@types/lodash": "^4.14.109",
    "@types/lusca": "^1.5.0",
    "@types/morgan": "^1.7.35",
    "@types/node": "^10.1.2",
    "@types/nodemailer": "^4.6.0",
    "@types/puppeteer": "^1.3.2",
    "@types/request": "^2.47.0",
    "@types/shelljs": "^0.8.0",
    "@types/source-map-support": "^0.4.0",
    "@types/supertest": "^2.0.4",
    "@types/winston": "^2.3.7",
    "axios": "^0.18.0",
    "chai": "^4.1.2",
    "concurrently": "^3.5.1",
    "crypto-js": "^3.1.9-1",
    "jest": "^23.0.0",
    "nodemon": "^1.13.0",
    "serverless-types": "0.0.1",
    "shelljs": "^0.8.2",
    "supertest": "^3.1.0",
    "ts-jest": "^22.0.4",
    "ts-node": "^5.0.0",
    "tslint": "^5.9.1",
    "typescript": "^2.8.3"
  }
}
